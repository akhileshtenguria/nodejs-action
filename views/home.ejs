<!DOCTYPE html>
<html>
<head>
  <title>Product List</title>
</head>
<body>
  <h1>Product List</h1>

  <% if (products.length === 0) { %>
    <p>No products available.</p>
  <% } else { %>
    <ul>
      <% products.forEach(product => { %>
        <li>
          <strong><%= product.name %></strong> - $<%= product.price %>
        </li>
      <% }) %>
    </ul>
  <% } %>
</body>
</html>
